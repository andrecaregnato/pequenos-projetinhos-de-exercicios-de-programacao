inicio
variavel real valor_1, valor_2, valor_3, media
escrever "Digite o primeiro valor: "
ler valor_1
escrever "Digite o segundo valor: "
ler valor_2
escrever "Digite o terceiro valor: "
ler valor_3
media <- (valor_1 + valor_2 + valor_3)/3
escrever "O valor da média desses três números é: ", media 
fim
